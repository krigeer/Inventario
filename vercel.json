{
  "builds": [
    {
      "src": "build.sh",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "cache-control": "public,max-age=31536000,immutable"
      },
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "inventario/wsgi.py"
    }
  ]
}
