{
  "builds": [
    {
      "src": "inventario/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "postBuildCommand": "python manage.py collectstatic --noinput"
      }
    }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/(.*)", "dest": "inventario/wsgi.py" }
  ]
}
